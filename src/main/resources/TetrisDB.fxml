<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.Pane?>

<AnchorPane xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="main.java.com.app.TetrisDBController">
   <children>
      <Pane layoutX="-56.0" prefHeight="400.0" prefWidth="543.0">
         <children>
            <Label layoutX="160.0" layoutY="125.0" text="User Name:" />
            <TextField fx:id="txtName" layoutX="248.0" layoutY="120.0">
               <effect>
                  <Blend />
               </effect>
            </TextField>
            <Button layoutX="277.0" layoutY="280.0" mnemonicParsing="false" onAction="#insertUsernameTetris" text="Save" />
            <Label layoutX="157.0" layoutY="175.0" text="Game Score:" />
            <Label layoutX="160.0" layoutY="223.0" text="Total Score:" />
            <TextField fx:id="totalScoreTxt" disable="true" layoutX="248.0" layoutY="218.0" text="0" />
            <Label layoutX="212.0" layoutY="75.0" text="Insert Your Name to save points!" />
            <TextField fx:id="gameScoreTxt" disable="true" layoutX="248.0" layoutY="170.0" text="0" />
         </children>
      </Pane>
   </children>
</AnchorPane>
